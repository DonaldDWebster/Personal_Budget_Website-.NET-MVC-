<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BudgetBuddy</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    
    <link rel="stylesheet" href="~/MIS421FinalProjectGit/wwwroot/lib/bootstrap/dist/js/bootstrap.js" asp-append-version="true" />

</head>


<body>
    @*<header>*@
        <div class="bg-dark p-4">
      <ul class="nav nav-white nav-pills">
        <li class="nav-item">
          <a class="nav-link active" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
        </li>
   
        <partial name="_LoginPartial" />
    
        
      </ul>
    </div>
  @*  </header>*@


<div class="text-center font-size-4 shadow-primary-lg bg-primary text-white p-6 mb-7">@ViewData["Title"]</div>
    
      
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

     <!-- ========== FOOTER ========== -->
    <footer class="">
        <div class="bg-gray-4000">
            <div class="container px-md-5">
                <div class="d-flex flex-wrap align-items-center pt-5 pb-3d border-gray-4100">
                    <ul class="list-unstyled mx-n3 mb-0 d-flex flex-wrap align-items-center">
                        <li class="px-3">
                            <a href="#" class="text-gray-1300 d-flex flex-wrap align-items-center"><i class="fab fa-facebook-f fa-inverse"></i> <span class="ml-2">Facebook</span></a>
                        </li>
                        <li class="px-3">
                            <a href="#" class="text-gray-1300 d-flex flex-wrap align-items-center"><i class="fab fa-twitter fa-inverse"></i> <span class="ml-2">Twitter</span></a>
                        </li>
                        <li class="px-3">
                            <a href="#" class="text-gray-1300 d-flex flex-wrap align-items-center"><i class="fab fa-google-plus-g fa-inverse"></i> <span class="ml-2">Google+</span></a>
                        </li>
                        <li class="px-3">
                            <a href="#" class="text-gray-1300 d-flex flex-wrap align-items-center"><i class="fab fa-vimeo-v fa-inverse"></i> <span class="ml-2">Vimeo</span></a>
                        </li>
                        <li class="px-3">
                            <a href="#" class="text-gray-1300 d-flex flex-wrap align-items-center"><i class="fas fa-rss fa-inverse"></i> <span class="ml-2">RSS</span></a>
                        </li>
                    </ul>
                </div>
                 
                     <div class="row pb-1">
                    <div class="col-md mb-5 mb-md-0">
                        <h4 class="font-size-18 font-weight-medium mb-2 text-gray-4200">Our services</h4>
                        <ul class="column-count-2 v2 list-unstyled mb-0">
                            <li class="py-1d">
                                <a class="h-g-white" href="Bills">Bills</a>
                            </li>
                            <li class="py-1d">
                                <a class="h-g-white" href="Investments">Investment</a>
                            </li>
                            <li class="py-1d">
                                <a class="h-g-white" href="Transactions">Transactions</a>
                            </li>
                            <li class="py-1d">
                                <a class="h-g-white" href="Budgets">Budget Management</a>
                            </li>
                        </ul>
                    </div>
                   
                    <div class="col-md-2 mb-1 mb-md-0 border-left border-gray-4100">
                        <div class="ml-1">
                            <h4 class="font-size-18 font-weight-medium mb-2 text-gray-4200">About BudgetBuddy</h4>
                            <ul class="list-unstyled mb-0">
                                <li class="py-1d">
                                    <a class="h-g-white" href="../static/contact.html">About Us</a>
                                </li>
                                <li class="py-1d">
                                    <a class="h-g-white" href="../static/contact.html">Careers at CompanyName</a>
                                </li>
                                <li class="py-1d">
                                    <a class="h-g-white" href="../static/contact.html">Corporate Responsibility</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="bg-gray-4300">
            <div class="container px-md-2">
                <div class="text-center d-md-flex flex-wrap align-items-center py-3">
                    <div class="font-size-13 text-gray-1300 mb-1 mb-md-0">Copyright © 2020, BudgetBuddy. All Rights Reserved</div>
                    <a href="../static/contact.html" class="font-size-13 h-g-white ml-md-auto">Privacy Policy</a>
                </div>
            </div>
        </div>
    </footer>
    <!-- ========== END FOOTER ========== -->

     <!-- JS Global Compulsory -->
    <script src="/assets/vendor/jquery/dist/jquery.min.js"></script>
    <script src="/assets/vendor/jquery-migrate/dist/jquery-migrate.min.js"></script>
    <script src="/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    

    <!-- JS Vodi -->
    <script src="/assets/js/hs.core.js"></script>
    <script src="/assets/js/hs.validation.js"></script>
    <script src="/assets/js/hs.cubeportfolio.js"></script>
    <script src="/assets/js/hs.slick-carousel.js"></script>
    <script src="/assets/js/hs.fancybox.js"></script>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
