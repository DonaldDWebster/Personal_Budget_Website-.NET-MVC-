@model IEnumerable<MIS421FinalProjectGit.Models.AdminAccounts>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AdminFName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdminLName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdminEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdminLastLogin)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AdminFName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminLName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminLastLogin)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AdminID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AdminID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AdminID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
